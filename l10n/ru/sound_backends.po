msgid "ALSA sound system backend"
msgstr "Поддержка подсистемы ALSA"

msgid "Error in ALSA backend: {}."
msgstr "Ошибка в подсистеме ALSA: {}."

msgid "Error in ALSA backend while working with device '{0}': {1}."
msgstr "Ошибка при работе с ALSA устройством '{0}': {1}."

